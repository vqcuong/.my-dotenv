	"editor.tokenColorCustomizations": {
		"[Dracula At Night]": {
			"textMateRules": [
				{
					"scope": "source",
					"settings": {
						"foreground": "#faffa3",
					}
				},
				{
					"scope": [
						"comment",
					],
					"settings": {
						"foreground": "#4b7cb0",
						"fontStyle": "italic"
					}
				},
				{
					"scope": [
						"comment.block",
						"comment.block.documentation"
					],
					"settings": {
						"foreground": "#774bb0",
						"fontStyle": "italic"
					}
				},
				{
					"scope": [
						"keyword",
						"keyword.control",
						"keyword.operator",
						"storage.type",
						"storage.modifier",
					],
					"settings": {
						"foreground": "#ff2877",
						"fontStyle": "italic"
					}
				},
				{
					"scope": [
						"string.quoted.docstring"
					],
					"settings": {
						"foreground": "#2c6b56"
					}
				},
				{
					"scope": [
						"string",
						"punctuation.definition.string.begin",
						"punctuation.definition.string.end",
						"punctuation.separator",
						"punctuation.separator.continuation",
						"punctuation.terminator",
					],
					"settings": {
						"foreground": "#ff7048",
					}
				},
				{
					"scope": "support.type.property-name",
					"settings": {
						"foreground": "#87e3ff",
						"fontStyle": "bold",
					}
				},
				{
					"scope": [
						"entity.name.class",
						"entity.name.type.class"
					],
					"settings": {
						"foreground": "#00fbff",
						"fontStyle": "bold"
					}
				},
				{
					"scope": [
						"entity.name.import",
					],
					"settings": {
						"foreground": "#8cff00",
						"fontStyle": "italic"
					}
				},
				{
					"scope": [
						"entity.alias",
					],
					"settings": {
						"foreground": "#f187ff",
						"fontStyle": "italic"
					}
				},
				{
					"scope": [
						"entity.name.function",
						"entity.name.method",
						"meta.function",
						"meta.function-call",
						"meta.function-call.generic",
						"keyword.operator.function.infix",
					],
					"settings": {
						"foreground": "#0084ff"
					}
				},
				{
					"scope": "variable.parameter",
					"settings": {
						"foreground": "#00ff5e",
						"fontStyle": "italic",
					}
				},
				{
					"scope": [
						"variable.parameter.function.language.special"
					],
					"settings": {
						"foreground": "#8cff00"
					}
				},
				{
					"scope": [
						"variable.other.property",
						"variable.other.property.static"
					],
					"settings": {
						"foreground": "#ffff00"
					}
				},
			]
		},
		"textMateRules": [
			{
				"scope": "keyword.other.dotenv",
				"settings": {
					"foreground": "#00000000"
				}
			}
		]
	},
